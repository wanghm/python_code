node {
    stage('SCM Checkout'){
        sh "echo SCM Checkout OK."
    }
    stage('Mvn Package'){
        sh "echo Mvn Package OK."
    }
    stage('Compile'){
        sh "echo compile OK."
    }
    stage('build image'){
        sh "echo build image OK."
    }
    stage('push image'){
        sh "echo push image OK."
    }
    stage('deploy'){
        sh "echo deploy OK."
    }
    
}
